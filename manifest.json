{
    "manifest_version": 2,
    "name": "Time to Work!",
    "version": "0.0.1",

	"browser_action": {
		"browser_style": true,
		"default_icon":{
			"48": "icon/48.png",
            "256": "icon/256.png"
		},
		"default_title": "Time to Work!",
		"default_popup": "/popup.html"
	},

    "permissions": [
        "storage"
    ],

	"options_ui": {
        "page": "analytics.html",
        "open_in_tab": true
    }


}